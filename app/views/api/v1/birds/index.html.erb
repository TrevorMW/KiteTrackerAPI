<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Birds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_birds.each do |api_v1_bird| %>
      <tr>
        <td><%= link_to 'Show', api_v1_bird %></td>
        <td><%= link_to 'Edit', edit_api_v1_bird_path(api_v1_bird) %></td>
        <td><%= link_to 'Destroy', api_v1_bird, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bird', new_api_v1_bird_path %>
