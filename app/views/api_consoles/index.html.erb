<p id="notice"><%= notice %></p>

<h1>Listing Api Consoles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_consoles.each do |api_console| %>
      <tr>
        <td><%= link_to 'Show', api_console %></td>
        <td><%= link_to 'Edit', edit_api_console_path(api_console) %></td>
        <td><%= link_to 'Destroy', api_console, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api console', new_api_console_path %>
